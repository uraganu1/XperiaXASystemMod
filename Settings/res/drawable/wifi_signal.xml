<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android"
  xmlns:settings="http://schemas.android.com/apk/res/com.android.settings">
    <item settings:state_encrypted="true">
        <level-list>
            <item android:drawable="@drawable/ic_wifi_lock_signal_0_light" android:maxLevel="0" />
            <item android:drawable="@drawable/ic_wifi_lock_signal_1_light" android:maxLevel="1" />
            <item android:drawable="@drawable/ic_wifi_lock_signal_2_light" android:maxLevel="2" />
            <item android:drawable="@drawable/ic_wifi_lock_signal_3_light" android:maxLevel="3" />
            <item android:drawable="@drawable/ic_wifi_lock_signal_4_light" android:maxLevel="4" />
        </level-list>
    </item>
    <item settings:state_encrypted="false">
        <level-list>
            <item android:drawable="@drawable/ic_wifi_signal_0_light" android:maxLevel="0" />
            <item android:drawable="@drawable/ic_wifi_signal_1_light" android:maxLevel="1" />
            <item android:drawable="@drawable/ic_wifi_signal_2_light" android:maxLevel="2" />
            <item android:drawable="@drawable/ic_wifi_signal_3_light" android:maxLevel="3" />
            <item android:drawable="@drawable/ic_wifi_signal_4_light" android:maxLevel="4" />
        </level-list>
    </item>
</selector>