<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen android:persistent="false" android:title="@string/notification_settings" android:orderingFromXml="true" settings:keywords="@string/keywords_sounds_and_notifications"
  xmlns:android="http://schemas.android.com/apk/res/android"
  xmlns:settings="http://schemas.android.com/apk/res/com.android.settings">
    <com.sonymobile.settings.preference.IntentPreference android:key="audiosettings" settings:before="first" settings:targetParentKey="sound" settings:hasMetaData="true">
        <intent android:targetPackage="com.sonyericsson.soundenhancement" android:action="android.intent.action.MAIN" android:targetClass="com.sonyericsson.soundenhancement.AudioEffectMenuForAudioActivity">
            <extra android:name="android.media.extra.PACKAGE_NAME" android:value="com.android.settings" />
            <extra android:name="com.sonyericsson.soundenhancement.EXTRA_START_TYPE" android:value="START_FOR_AUDIO" />
        </intent>
    </com.sonymobile.settings.preference.IntentPreference>
    <com.sonymobile.settings.preference.IntentPreference android:key="accessorysettings" settings:before="predefine" settings:hasMetaData="true">
        <intent android:targetPackage="com.sonyericsson.soundenhancement" android:action="android.intent.action.MAIN" android:targetClass="com.sonyericsson.soundenhancement.AudioEffectMenuForAccessoryActivity">
            <extra android:name="android.media.extra.PACKAGE_NAME" android:value="com.android.settings" />
            <extra android:name="com.sonyericsson.soundenhancement.EXTRA_START_TYPE" android:value="START_FOR_ACCESSORY" />
        </intent>
    </com.sonymobile.settings.preference.IntentPreference>
    <PreferenceCategory android:title="@string/audio_profile_category_predefine_title" android:key="predefine" />
    <com.mediatek.audioprofile.AudioProfilePreference android:title="@string/general_settings_title" android:key="mtk_audioprofile_general" android:summary="@string/ring_summary" />
    <com.mediatek.audioprofile.AudioProfilePreference android:title="@string/silent_settings_title" android:key="mtk_audioprofile_silent" android:summary="@string/silent_summary" />
    <com.mediatek.audioprofile.AudioProfilePreference android:title="@string/meeting_settings_title" android:key="mtk_audioprofile_meeting" android:summary="@string/meeting_summary" />
    <com.mediatek.audioprofile.AudioProfilePreference android:title="@string/outdoor_settings_title" android:key="mtk_audioprofile_outdoor" android:summary="@string/outdoor_summary" />
    <PreferenceCategory android:title="@string/audio_profile_others" android:key="notification" />
    <PreferenceScreen android:title="@string/notification_section_header" android:key="notification_screen" android:fragment="com.mediatek.audioprofile.AudioProfileNotification" />
    <PreferenceScreen android:title="@string/zen_mode_settings_title" android:key="zen_mode" android:fragment="com.android.settings.notification.ZenModeSettings" settings:keywords="@string/keywords_sounds_and_notifications_interruptions" />
    <Preference android:persistent="false" android:title="@string/manage_zen_access_title" android:key="manage_zen_access" android:fragment="com.android.settings.notification.ZenAccessSettings" />
    <PreferenceScreen android:title="@string/sound_enhancement_title" android:key="sound_enhancement" android:fragment="com.mediatek.audioprofile.SoundEnhancement" />
</PreferenceScreen>