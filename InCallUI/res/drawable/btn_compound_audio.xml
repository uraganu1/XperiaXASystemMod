<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@id/compoundBackgroundItem" android:drawable="@drawable/btn_compound_background" />
    <item android:id="@id/moreIndicatorItem">
        <bitmap android:gravity="center" android:src="@drawable/clh_large_icbp_arrow_whitespace" android:tint="@color/selectable_icon_tint" />
    </item>
    <item android:id="@id/bluetoothItem">
        <bitmap android:gravity="center" android:src="@drawable/clh_large_icbp_bt_connected_bluetooth" android:tint="@color/selectable_icon_tint" />
    </item>
    <item android:id="@id/handsetItem">
        <bitmap android:gravity="center" android:src="@drawable/clh_large_icbp_bt_connected_phonespeaker" android:tint="@color/selectable_icon_tint" />
    </item>
    <item android:id="@id/speakerphoneItem">
        <bitmap android:gravity="center" android:src="@drawable/clh_large_icbp_speaker" android:tint="@color/selectable_icon_tint" />
    </item>
</layer-list>